<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_3.xsd"
             version="1.3">
    <webservice-description>
        <webservice-description-name>pack-ws</webservice-description-name>
        <port-component>
            <port-component-name>pack-ws</port-component-name>
            <wsdl-service xmlns:pfx="http://www.bbva.com.pe/pack-ws/">pfx:pack-ws</wsdl-service>
            <wsdl-port xmlns:pfx="http://www.bbva.com.pe/pack-ws/">pfx:pack-wsSOAP</wsdl-port>
            <enable-mtom>false</enable-mtom>
            <service-endpoint-interface>com.bbva.packws.webservice.endpoints.PackWs</service-endpoint-interface>
            <service-impl-bean>
                <servlet-link>com.bbva.packws.webservice.endpoints.impl.PackWsSOAPImpl</servlet-link>
            </service-impl-bean>
        </port-component>
    </webservice-description>
</webservices>
